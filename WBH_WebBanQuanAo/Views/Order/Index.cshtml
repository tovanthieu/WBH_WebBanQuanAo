        /* Đảm bảo form được căn giữa */
        .theme-card {
            padding: 20px;
            border: 1px solid #ddd;
            border-radius: 5px;
        }

        /* Khoảng cách giữa các form-row */
        .form-row.row {
            margin-bottom: 20px;
        }

        /* Căn giữa nút */
        .btn_change_pass, .btn_change_pass_update {
            display: inline-block;
